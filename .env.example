# Environment variables for Advanced Prompting Techniques
# Copy this file to .env and fill in your actual values

# ============================================================================
# REQUIRED: Gemini API Configuration
# ============================================================================
# Get your API key from: https://aistudio.google.com/
GEMINI_API_KEY=your-gemini-api-key-here

# Optional: Override default model
GEMINI_MODEL=gemini-2.5-flash

# ============================================================================
# Advanced Prompting Configuration
# ============================================================================
# Default temperature for response generation (0.0 to 1.0)
DEFAULT_TEMPERATURE=0.7

# Default thinking budget for reasoning tasks
DEFAULT_THINKING_BUDGET=5000

# Maximum tokens for response generation
MAX_TOKENS=2048

# ============================================================================
# Testing Configuration
# ============================================================================
# Enable quick test mode for faster execution
QUICK_TEST_MODE=false

# Maximum API calls per test (to avoid rate limits)
MAX_API_CALLS_PER_TEST=2

# API call timeout in seconds
API_TIMEOUT=10

# ============================================================================
# Performance Optimization
# ============================================================================
# Enable response caching (if implemented)
CACHE_ENABLED=false

# Cache time-to-live in seconds
CACHE_TTL=3600

# Request timeout in seconds
REQUEST_TIMEOUT=30

# ============================================================================
# Rate Limiting Configuration
# ============================================================================
# Gemini Free Tier: 10 requests per minute
# Enable rate limiting protection
RATE_LIMIT_ENABLED=true

# Delay between requests (seconds)
REQUEST_DELAY=6

# Maximum retries for rate-limited requests
MAX_RETRIES=3

# ============================================================================
# Technique-Specific Settings
# ============================================================================
# Few-shot Learning
FEW_SHOT_TEMPERATURE=0.3
FEW_SHOT_MAX_EXAMPLES=5

# Chain-of-Thought
COT_TEMPERATURE=0.3
COT_THINKING_BUDGET=10000

# Tree-of-Thought
TOT_TEMPERATURE=0.6
TOT_MAX_APPROACHES=3
TOT_THINKING_BUDGET=8000

# Self-Consistency
SC_TEMPERATURE=0.7
SC_NUM_SAMPLES=5
SC_THINKING_BUDGET=5000

# Meta-Prompting
META_TEMPERATURE=0.4
META_THINKING_BUDGET=8000

# ============================================================================
# Development and Debugging
# ============================================================================
# Enable debug logging
DEBUG_MODE=false

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Enable verbose output
VERBOSE_OUTPUT=false

# ============================================================================
# Setup Instructions:
# ============================================================================
# 1. Copy this file: cp .env.example .env
# 2. Get your Gemini API key from: https://aistudio.google.com/
# 3. Replace 'your-gemini-api-key-here' with your actual API key
# 4. Optionally customize other settings above
# 5. Save the .env file
# 6. Run: python main.py --technique few-shot
# 7. Run tests: python testsss.py

# ============================================================================
# Usage Examples:
# ============================================================================
# Test individual techniques:
# python main.py --technique few-shot
# python main.py --technique chain-of-thought
# python main.py --technique tree-of-thought

# Run specific examples:
# python main.py --technique few-shot --example sentiment
# python main.py --technique chain-of-thought --example math

# Run comprehensive tests:
# python testsss.py                    # Full test suite
# QUICK_TEST_MODE=true python testsss.py  # Fast test mode

# ============================================================================
# Security Notes:
# ============================================================================
# - Never commit your actual .env file to version control
# - Keep your API key secure and don't share it
# - The free tier has rate limits (10 requests/minute)
# - Use QUICK_TEST_MODE=true for development testing
# - Monitor your API usage at https://aistudio.google.com/

# ============================================================================
# Troubleshooting:
# ============================================================================
# Rate Limit Errors (429):
# - Wait 60 seconds between intensive operations
# - Use individual technique testing: --technique few-shot
# - Enable RATE_LIMIT_ENABLED=true
# - Increase REQUEST_DELAY value

# API Key Errors:
# - Ensure API key starts with 'AIza'
# - Check key is active at https://aistudio.google.com/
# - Verify no extra spaces in .env file

# Import Errors:
# - Run: pip install -r requirements.txt
# - Check Python version (3.8+ recommended)
# - Ensure all dependencies are installed